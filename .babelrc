{
   "presets":[
      [
         "@babel/preset-env",
         {
            "targets":{
               "browsers":[
                  "last 2 versions"
               ]
            },
            "modules":false,
            "forceAllTransforms":true,
            useBuiltIns: "usage"
         }
      ],
      [
         "@babel/preset-react",
         {
            useBuiltIns: "usage"
         }
      ]
   ],
   "plugins":[
      "@babel/plugin-proposal-function-bind",
      "@babel/plugin-proposal-class-properties",
      [
         "@babel/plugin-proposal-object-rest-spread",
         {
            "useBuiltIns":"usage"
         }
      ],
      [
         "react-intl",
         {
            "messagesDir":"./build/messages/"
         }
      ]
   ],
   "env":{
      "test":{
         "presets":[
            "@babel/preset-env",
            "@babel/preset-react"
         ]
      },
      "development":{
         "presets":[
            [
               "@babel/preset-env",
               {
                  "debug":true
               }
            ],
            [
               "@babel/preset-react",
               {
                  "development":true
               }
            ]
         ]
      }
   }
}